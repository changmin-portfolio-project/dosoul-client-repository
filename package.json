{
  "name": "dosoul-monorepo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev-user-service": "npm run dev --workspace=apps/user-web",
    "dev-admin-service": "npm run dev --workspace=apps/admin-web",
    "build": "tsc -b && vite build",
    "build:admin": "npm run build --workspace=apps/admin-web",
    "build:user": "npm run build --workspace=apps/user-web",
    "build:all": "npm run build:admin && npm run build:user",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.83.0",
    "@types/luxon": "^3.6.2",
    "@types/qs": "^6.14.0",
    "@types/react-datepicker": "^6.2.0",
    "@types/uuid": "^10.0.0",
    "@use-gesture/react": "^10.3.1",
    "axios": "^1.10.0",
    "luxon": "^3.7.1",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-datepicker": "^8.5.0",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.6.2",
    "react-spring": "^10.0.1",
    "recoil": "^0.7.7",
    "styled-components": "^6.1.18",
    "styled-reset": "^4.5.2",
    "swiper": "^11.2.10",
    "tua-body-scroll-lock": "^1.5.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "terser": "^5.44.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0"
  }
}
